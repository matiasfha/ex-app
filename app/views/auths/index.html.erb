<h1>Listing auths</h1>

<table>
  <tr>
    <th>User</th>
    <th>Provider</th>
    <th>Uid</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @auths.each do |auth| %>
  <tr>
    <td><%= auth.user_id %></td>
    <td><%= auth.provider %></td>
    <td><%= auth.uid %></td>
    <td><%= link_to 'Show', auth %></td>
    <td><%= link_to 'Edit', edit_auth_path(auth) %></td>
    <td><%= link_to 'Destroy', auth, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Auth', new_auth_path %>
